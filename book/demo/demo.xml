<odoo>
    <data>
        <record id="book_type_novel" model="book.type">
            <field name="name">Novel</field>
        </record>

        <record id="book_type_comic" model="book.type">
            <field name="name">Comic</field>
        </record>

        <record id="book_1" model="book.book">
            <field name="name">Book 1</field>
            <field name="author">Andi</field>
            <field name="number_of_pages">500</field>
            <field name="book_type_id" ref="book.book_type_novel"/>
        </record>

        <record id="book_2" model="book.book">
            <field name="name">Book 2</field>
            <field name="author">Dana</field>
            <field name="number_of_pages">100</field>
            <field name="book_type_id" ref="book.book_type_comic"/>
        </record>

        <record id="book_3" model="book.book">
            <field name="name">Book 3</field>
            <field name="author">Via</field>
            <field name="number_of_pages">450</field>
            <field name="book_type_id" ref="book.book_type_novel"/>
        </record>
    </data>
</odoo>
